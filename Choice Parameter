
pipeline {
    agent any
        options(
            [parameters([choice(choices: ["A", "B", "C"].join("\n"),
            description: 'Some choice parameter', 
            name: 'SOME_CHOICE')])])
        
        stages {
        stage('Example') {
            steps {               
                echo "Choice: ${params.CHOICE}"               
            }
        }
    }
        }
